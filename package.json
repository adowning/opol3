		"postcss-loader": "^2.0.7",
		"postcss-nested": "^2.1.2",
		"raw-loader": "^0.5.1",
		"rimraf": "^2.6.2",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.0",
		"url-loader": "^0.5.9",
		"vue-html-loader": "^1.2.4",
		"vue-loader": "^13.3.0",
